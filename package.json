    "@types/node": "^18.19.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "babel-jest": "^26.6.3",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
